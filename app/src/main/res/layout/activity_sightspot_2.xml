<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent" >
    
	<RelativeLayout 
	    android:layout_width="match_parent" android:layout_height="match_parent"
	    android:id="@+id/main">

	    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_row="1"
            android:layout_column="1"
            android:id="@+id/day1"
            android:layout_gravity="top">
            
         	<TextView
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:textAppearance="?android:attr/textAppearanceLarge"
               android:text="Day One"
               android:id="@+id/day1_textView"
               android:layout_alignParentTop="true"
               android:layout_alignParentLeft="true"
               android:layout_alignParentStart="true" />
         	
        </RelativeLayout>
	        
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_row="2"
            android:layout_column="3"
            android:id="@+id/morning"
            android:layout_below="@+id/day1">
    
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Morning"
                android:id="@+id/m_textView"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
    
            <TextView
                android:id="@+id/m_textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/m_textView"
                android:layout_toLeftOf="@+id/m_searchView"
                android:layout_toStartOf="@+id/m_searchView" />
    
            <ImageButton
                android:id="@+id/m_searchView"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/search"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/m_textView" />
            
            <Button
	            android:id="@+id/button_transit"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_alignParentBottom="true"
	            android:layout_alignParentRight="true"
	            android:text="Transit" />
    
        </RelativeLayout>
    
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="151dp"
            android:layout_row="2"
            android:layout_column="2"
            android:id="@+id/aternoon"
            android:layout_below="@+id/morning"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">
    
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Afternoon"
                android:id="@+id/a_textView"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
    
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/a_textView2"
                android:layout_below="@+id/a_textView"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_toLeftOf="@+id/a_searchView"
                android:layout_toStartOf="@+id/a_searchView" />
    
            <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/search"
                android:id="@+id/a_searchView"
                android:layout_below="@+id/a_textView"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
            
            <Button
	            android:id="@+id/button_transit2"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_alignParentBottom="true"
	            android:layout_alignParentRight="true"
	            android:text="Transit" />
        </RelativeLayout>
    
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_row="2"
            android:layout_column="3"
            android:layout_below="@+id/aternoon"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:id="@+id/night">
    
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Night"
                android:id="@+id/n_textView"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
    
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/n_textView2"
                android:layout_below="@+id/n_textView"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_toLeftOf="@+id/n_searchView"
                android:layout_toStartOf="@+id/n_searchView" />
    
            <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/search"
                android:id="@+id/n_searchView"
                android:layout_below="@+id/n_textView"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>
     
        
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_row="1"
            android:layout_column="1"
            android:id="@+id/day2"
            android:layout_below="@+id/night">
            <View
			    android:layout_width="fill_parent"
			    android:layout_height="1dp"
			    android:background="@android:color/darker_gray" />
         	<TextView
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:textAppearance="?android:attr/textAppearanceLarge"
               android:text="Day Two"
               android:id="@+id/day2_textView"
               android:layout_alignParentTop="true"
               android:layout_alignParentLeft="true"
               android:layout_alignParentStart="true" />
         	
        </RelativeLayout>
        
        
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_row="2"
            android:layout_column="3"
            android:id="@+id/morning2"
            android:layout_below="@+id/day2" >
    
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Morning"
                android:id="@+id/m2_textView"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
    
            <TextView
                android:id="@+id/m2_textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/m2_textView"
                android:layout_toLeftOf="@+id/m2_searchView"
                android:layout_toStartOf="@+id/m2_searchView" />

            <ImageButton
                android:id="@+id/m2_searchView"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/m2_textView"
                android:background="@drawable/search" />
            
           <Button
	            android:id="@+id/button_transit3"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_alignParentBottom="true"
	            android:layout_alignParentRight="true"
	            android:text="Transit" />
    
        </RelativeLayout>
    
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="151dp"
            android:layout_row="2"
            android:layout_column="2"
            android:id="@+id/aternoon2"
            android:layout_below="@+id/morning2"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">
    
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Afternoon"
                android:id="@+id/a2_textView"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
    
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/a2_textView2"
                android:layout_below="@+id/a2_textView"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_toLeftOf="@+id/a2_searchView"
                android:layout_toStartOf="@+id/a2_searchView" />
    
            <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/search"
                android:id="@+id/a2_searchView"
                android:layout_below="@+id/a2_textView"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
            
           <Button
	            android:id="@+id/button_transit4"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_alignParentBottom="true"
	            android:layout_alignParentRight="true"
	            android:text="Transit" />
        </RelativeLayout>
    
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_row="2"
            android:layout_column="3"
            android:layout_below="@+id/aternoon2"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:id="@+id/night2">
    
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Night"
                android:id="@+id/n2_textView"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
    
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/n2_textView2"
                android:layout_below="@+id/n2_textView"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_toLeftOf="@+id/n2_searchView"
                android:layout_toStartOf="@+id/n2_searchView" />
    
            <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/search"
                android:id="@+id/n2_searchView"
                android:layout_below="@+id/n2_textView"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>
        
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_row="2"
            android:layout_column="3"
            android:layout_below="@+id/night2"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:id="@+id/map">

            <Button
                android:id="@+id/button_final"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="Map View" />
          
        </RelativeLayout>
        
    
	</RelativeLayout>
</ScrollView>
    